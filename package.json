{
  "name": "azama-box",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jin-K/azama-box.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve client1",
    "start:client2": "ng serve client2",
    "build": "ng build client1",
    "build:client2": "ng build client2",
    "build:github-pages": "run-s build:libs build:docs",
    "build:lib:google-identity": "ng build @jin-k/google-identity",
    "build:lib:utils": "ng build @jin-k/utils",
    "build:lib:google-oidc-login-provider": "ng build @jin-k/google-oidc-login-provider",
    "build:libs": "run-s build:lib:*",
    "deploy": "run-s build:lib:google-identity build:lib:utils && ng deploy client1 && git fetch --all",
    "deploy:client2": "run-s build:lib:google-oidc-login-provider && ng deploy client2 && git fetch --all",
    "watch": "ng build client1 --watch --configuration development",
    "watch:client2": "ng build client2 --watch --configuration development",
    "watch:lib:google-identity": "ng build --watch --configuration development @jin-k/google-identity",
    "watch:lib:utils": "ng build --watch --configuration development @jin-k/utils",
    "watch:lib:google-oidc-login-provider": "ng build --watch --configuration development @jin-k/google-oidc-login-provider",
    "watch:libs": "run-p watch:lib:*",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@abacritt/angularx-social-login": "^1.2.1",
    "@angular/animations": "^14.0.3",
    "@angular/common": "^14.0.3",
    "@angular/compiler": "^14.0.3",
    "@angular/core": "^14.0.3",
    "@angular/forms": "^14.0.3",
    "@angular/platform-browser": "^14.0.3",
    "@angular/platform-browser-dynamic": "^14.0.3",
    "@angular/router": "^14.0.3",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.3",
    "@angular/cli": "^14.0.3",
    "@angular/compiler-cli": "^14.0.3",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^1.0.0",
    "angular-oauth2-oidc": "^13.0.1",
    "http-server": "^14.1.1",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^14.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "2.7.1",
    "typescript": "~4.7.4"
  }
}
